---
import Layout from "../layouts/Layout.astro";
import List from "../components/List.astro";
import MyHtmlCom from "../components/MyHtmlCom.html";
import MyReactComA from "../components/MyReactComA";
// import MyReactComB from "../components/MyReactComB";
// import MyReactComC from "../components/MyReactComC";
import MyVueCom from "../components/MyVueCom.vue";
import MyMdCom from "../components/MyMdCom.md";
---

<Layout title="Welcome to Astro.">
    <a href="/about">前往About</a>
    <main id="index">
        <MyHtmlCom />
        <MyVueCom client:load>
            <List />
            <MyReactComA client:load title="client:load" />
        </MyVueCom>
        <!-- <MyReactComB client:only title="client:only" /> -->
        <!-- <MyReactComC client:only="svelte" title='client:only="svelte"' /> -->

        <MyMdCom />
    </main>
</Layout>
